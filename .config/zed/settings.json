{
  "language_models": {
    "openai_compatible": {
      "TogetherAI": {
        "api_url": "https://api.together.xyz/v1",
        "available_models": [
          {
            "name": "deepseek-ai/DeepSeek-V3.1",
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 2000,
            "capabilities": {
              "tools": true,
              "images": false,
              "parallel_tool_calls": false,
              "prompt_cache_key": false,
              "chat_completions": true,
            },
          },
          {
            "name": "zai-org/GLM-4.7",
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 2000,
            "capabilities": {
              "tools": true,
              "images": false,
              "parallel_tool_calls": false,
              "prompt_cache_key": false,
              "chat_completions": true,
            },
          },
          {
            "name": "Qwen/Qwen3-Coder-480B-A35B-Instruct-FP8",
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 2000,
            "capabilities": {
              "tools": true,
              "images": false,
              "parallel_tool_calls": false,
              "prompt_cache_key": false,
              "chat_completions": true,
            },
          },
          {
            "name": "moonshotai/Kimi-K2.5",
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 2000,
            "capabilities": {
              "tools": true,
              "images": false,
              "parallel_tool_calls": false,
              "prompt_cache_key": false,
              "chat_completions": true,
            },
          },
        ],
      },
    },
  },
  "features": {
    "edit_prediction_provider": "zed",
  },
  "agent": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-opus-4-5-thinking",
    },
    "model_parameters": [],
  },
  "base_keymap": "VSCode",
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "disable_ai": false,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "One Dark",
  },
  "search": {
    "whole_word": false,
    "case_sensitive": false,
    "regex": false,
  },
}
